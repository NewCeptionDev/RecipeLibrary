{
  "name": "recipe-library",
  "version": "0.0.1",
  "author": {
    "name": "Julius Schmidt",
    "email": "newceptiondev@googlemail.com"
  },
  "main": "app/main.js",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "ng:serve": "ng serve -o",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.electron.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build && electron .",
    "prettier": "prettier --write .",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "format": "npm run prettier && npm run eslint:fix"
  },
  "prettier": "@newceptiondev/prettier-config",
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0-next.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.0.0-next.0",
    "@angular/compiler": "^14.0.0-next.0",
    "@angular/core": "^14.0.0-next.0",
    "@angular/forms": "^14.0.0-next.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^14.0.0-next.0",
    "@angular/platform-browser-dynamic": "^14.0.0-next.0",
    "@angular/router": "^14.0.0-next.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.0",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "^14.0.0-next.0",
    "@newceptiondev/eslint-config": "1.0.2",
    "@newceptiondev/prettier-config": "1.0.4",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~4.0.0",
    "electron": "^20.1.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "typescript": "~4.7.2",
    "wait-on": "^6.0.1"
  }
}
