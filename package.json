{
  "name": "recipe-library",
  "productName": "Recipe Library",
  "version": "1.4.2",
  "author": {
    "name": "Julius Schmidt",
    "email": "newceptiondev@googlemail.com"
  },
  "homepage": "https://github.com/NewCeptionDev/RecipeLibrary",
  "license": "MIT",
  "description": "Recipe Library, making it easy to find Recipes based on Ingredients, Cookbook or Categories",
  "main": "app/main.js",
  "scripts": {
    "ng": "ng",
    "start:local": "npm-run-all -p electron:serve ng:serve",
    "start": "electron-forge start",
    "ng:serve": "ng serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --source-map=false --browsers=ChromeHeadless --watch=false",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.electron.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build && electron .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "format": "npm run prettier && npm run eslint:fix && npm run prettier",
    "lint": "ng lint",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@angular/animations": "^14.0.0-next.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.0.0-next.0",
    "@angular/compiler": "^14.0.0-next.0",
    "@angular/core": "^14.0.0-next.0",
    "@angular/forms": "^14.0.0-next.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^14.0.0-next.0",
    "@angular/platform-browser-dynamic": "^14.0.0-next.0",
    "@angular/router": "^14.0.0-next.0",
    "electron-squirrel-startup": "^1.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "^14.0.0-next.0",
    "@cypress/schematic": "^2.5.1",
    "@electron-forge/cli": "^7.3.1",
    "@electron-forge/maker-deb": "^7.3.1",
    "@electron-forge/maker-dmg": "^7.3.1",
    "@electron-forge/maker-squirrel": "^7.3.1",
    "@electron-forge/maker-zip": "^7.3.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.1",
    "@electron-forge/publisher-github": "^7.3.1",
    "@newceptiondev/eslint-config": "1.0.2",
    "@newceptiondev/prettier-config": "1.0.4",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "cypress": "latest",
    "electron": "^22.3.23",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "typescript": "~4.7.2",
    "wait-on": "^7.2.0"
  }
}
